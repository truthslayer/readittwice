$(window).on('load', function() {
    $(function(){
    $('.location').each(function() {
        var timeZone = $(this).data('tz');
        var now = moment().tz("America/New_York");
        var mins = now.minutes();
     if (mins < 15) {
           now.subtract(1, 'hours');
      }
     var hrs = now.hours();
     var rounded = hrs < 6 ? 0 : hrs < 9? 6 :  hrs < 12 ? 9 : hrs < 18 ? 12 : hrs < 21 ? 18 : hrs < 23 ? 21 : 23 ;
     now.hours(rounded);
     var yr = now.year();
     var mon = now.months();
     var day = now.dates();
     $("#cnn").attr("src", "news-clips/"+ now.format("YYYY-MM-DD") + "." + now.hours() + "/www.cnn.com-phantomjs.pdf#toolbar=0&amp;scrollbar=0");
     $("#cnnl").attr("src", "news-clips/"+ now.format("YYYY-MM-DD") + "." + now.hours() + "/www.cnn.com-phantomjs.pdf#toolbar=0&amp;scrollbar=0");
     $("#fox").attr("src", "news-clips/"+ now.format("YYYY-MM-DD") + "." + now.hours() + "/www.foxnews.com-wkh.pdf#toolbar=0&amp;scrollbar=0&amp;zoom=45");
     $("#foxl").attr("src", "news-clips/"+ now.format("YYYY-MM-DD") + "." + now.hours() + "/www.foxnews.com-wkh.pdf#toolbar=0&amp;scrollbar=0&amp;zoom=45");
     $("#wsj").attr("src", "news-clips/"+ now.format("YYYY-MM-DD") + "." + now.hours() + "/www.wsj.com-wkh.pdf#toolbar=0&amp;scrollbar=0&amp;zoom=45");
     $("#wsjl").attr("src", "news-clips/"+ now.format("YYYY-MM-DD") + "." + now.hours() + "/www.wsj.com-wkh.pdf#toolbar=0&amp;scrollbar=0&amp;zoom=45");
     $("#nyt").attr("src", "news-clips/"+ now.format("YYYY-MM-DD") + "." + now.hours() + "/www.nytimes.com-wkh.pdf#toolbar=0&amp;scrollbar=0&amp;zoom=45");
