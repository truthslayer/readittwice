		$("#wsjl").attr("src", hprefix +  dt_now() + "/www.wsj.com-phantomjs.pdf#toolbar=0&amp;scrollbar=0&amp;zoom=45");
           } else {
                $("#cnn").attr("src", hprefix + this.value + ".23/www.cnn.com-phantomjs.pdf#toolbar=0&amp;scrollbar=0");
