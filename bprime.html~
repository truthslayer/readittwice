      function display(msg) {
	  $("<p>").html(msg).appendTo(document.body);
      }
    var prefix = "news-clips/";
    var hprefix = "news-clips/";
    function dt_now() {
	var timeZone = $(this).data('tz');
	var now = moment().tz("America/New_York");
	var mins = now.minutes();
	if (mins < 15) {
	    now.subtract(1, 'hours');
	}
	var hrs = now.hours();
        var rounded = hrs < 6 ? 0 : hrs < 9? 6 :  hrs < 12 ? 9 : hrs < 18 ? 12 : hrs < 21 ? 18 : hrs < 23 ? 21 : 23 ;
	if (rounded == 0) {
		now.subtract(1, 'days');
		rounded = 23;
        }
	now.hours(rounded);
	var yr = now.year();
	var mon = now.months();
	var day = now.date();
	display(now.format("YYYY-MM-DD") + "." + now.hours());											       
	return(now.format("YYYY-MM-DD") + "." + now.hours());
    }

  function today(td){
      var d = new Date();
      return td.getDate() == d.getDate() && td.getMonth() == d.getMonth() && td.getFullYear() == d.getFullYear();
  }

    function tweek(td){
	var d = new Date();
	var tdn = td.getDate();
	var dn = d.getDate();
	return tdn <= dn && tdn >= dn -7;
    }
    
